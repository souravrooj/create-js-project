      "@typescript-eslint/eslint-plugin": "^6.13.0",
      "@typescript-eslint/parser": "^6.13.0",
      "eslint": "^8.54.0",
      "jest": "^29.7.0",
      "prettier": "^3.1.0",
      "ts-jest": "^29.1.1",
      "ts-node": "^10.9.1",
      "typescript": "^5.3.2"
    },
  "engines": {
    "node": ">=14.0.0"
  }
