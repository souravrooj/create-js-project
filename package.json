  "repository": {
    "type": "git",
    "url": "https://github.com/souravrooj/create-js-project.git"
  },
  "bugs": {
    "url": "https://github.com/souravrooj/create-js-project/issues"
  },
  "homepage": "https://github.com/souravrooj/create-js-project#readme",
  "dependencies": {
    "commander": "^11.1.0",
    "fs-extra": "^11.1.1",
    "chalk": "^4.1.2",
    "inquirer": "^8.2.6"
