{
  "name": "js-project-generator",
  "version": "1.1.0",
  "description": "CLI tool to create various JavaScript/TypeScript project structures",
  "main": "dist/index.js",
  "bin": {
    "js-project-generator": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "cli",
    "generator",
    "javascript",
    "typescript",
    "nodejs",
    "express",
    "nextjs",
    "react",
    "nest",
    "electron",
    "react-native"
  ],
  "author": {
